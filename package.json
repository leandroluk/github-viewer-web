{
  "name": "@github-viewer/web",
  "displayName": "Github Viewer - WEB",
  "version": "0.0.1",
  "description": "Frontend criado para portif√≥lio em ReactJS",
  "homepage": "https://github.com/leandroluk/github-viewer-web",
  "repository": "https://github.com/leandroluk/github-viewer-web.git",
  "author": {
    "email": "leandroluk@gmail.com",
    "name": "Leandro Santiago Gomes",
    "url": "https://www.linkedin.com/in/leandroluk/"
  },
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest --passWithNoTests --runInBand --detectOpenHandles --silent --noStackTrace",
    "test:v": "jest --passWithNoTests --runInBand --detectOpenHandles --verbose",
    "test:w": "npm run test -- --watch",
    "test:unit": "npm run test -- -c jest.unit.config.ts",
    "test:integration": "npm run test -- -c jest.integration.config.ts",
    "test:coverage": "npm run test -- --coverage",
    "test:coverage:unit": "npm run test:unit -- --coverage",
    "test:coverage:integration": "npm run test:integration -- --coverage",
    "test:staged": "npm test -- --findRelatedTests"
  },
  "dependencies": {
    "@mantine/core": "^7.2.2",
    "@mantine/form": "^7.2.2",
    "@mantine/hooks": "^7.2.2",
    "@mantine/notifications": "^7.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.19.0",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
